@{
    @model IEnumerable<QuintaEvaluacion.Models.Photo>
}

<div class="py-3">
    @if (Model.Count() == 0)
    {
        <p class="text-center ">No images to show...</p>
    }
    else
    {
        <p class="my-3">Latest added</p>
    }
    <div class="row row-cols-1 row-cols-md-4 g-4">
        @foreach (var photo in Model)
        {
            <div class="col">
                @Html.Partial("_PhotoItem", photo)
            </div>
        }
    </div>
</div>
